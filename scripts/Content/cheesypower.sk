on unload:
    loop {powerbank.location::*}:
        set {_n1} to 0
        loop (3, 4, and 5):
            add 1 to {_n1}
            set {powerbank.save::%loop-value-1%::%{_n1}%} to slot loop-value-2 of gui with id "powerbank.%loop-value-1%"
    loop {tidegen.location::*}:
        set {_n2} to 0
        loop (3, 4, and 5):
            add 1 to {_n2}
            set {tidegen.save::%loop-value-1%::%{_n2}%} to slot loop-value-2 of gui with id "TideGen.%loop-value-1%"
    loop {drinkblender.location::*}:
        set {_n3} to 0
        loop (0, 9, 18, 27, 36, 45, 12, 16, and 41):
            add 1 to {_n3}
            set {drinkblender.save::%loop-value-1%::%{_n3}%} to slot loop-value-2 of gui with id "DrinkBlender.%loop-value-1%"
    loop {magnet.location::*}:
        set {_n4} to 0
        loop (3, 4, and 5):
            add 1 to {_n4}
            set {magnet.save::%loop-value-1%::%{_n4}%} to slot loop-value-2 of gui with id "Magnet.%loop-value-1%"
    loop {engine.location::*}:
        set {_n5} to 0
        loop (0, 9, 18, and 13):
            add 1 to {_n5}
            set {engine.save::%loop-value-1%::%{_n5}%} to slot loop-value-2 of gui with id "SearingEngine.%loop-value-1%"
        
on load:
    loop {powerbank.location::*}:
        if gui with id "powerbank.%loop-value%" is not set:
            create a gui with id "powerbank.%loop-value%" with virtual dropper named "Power Bank" with shape "w_w" and "123" and "www":
                set {_n1} to 0
                loop 3 times:
                    add 1 to {_n1}
                    set {_n4::*} to 3, 4, and 5
                    if {powerbank.save::%loop-value-1%::%{_n1}%} is not set:
                        make gui slot {_n4::%{_n1}%} with moveable air
                    if {powerbank.save::%loop-value-1%::%{_n1}%} is air:
                        make gui slot {_n4::%{_n1}%} with moveable air
                    else:
                        make gui slot {_n4::%{_n1}%} with moveable {powerbank.save::%loop-value-1%::%{_n1}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
    loop {tidegen.location::*}:
        if gui with id "TideGen.%loop-value%" is not set:
            create a gui with id "TideGen.%loop-value%" with virtual dropper named "Tide Pool Generator" with shape "w_w" and "123" and "www":
                set {_n2} to 0
                loop 3 times:
                    add 1 to {_n2}
                    set {_n5::*} to 3, 4, and 5
                    if {tidegen.save::%loop-value-1%::%{_n2}%} is not set:
                        make gui slot {_n5::%{_n2}%} with moveable air
                    if {tidegen.save::%loop-value-1%::%{_n2}%} is air:
                        make gui slot {_n5::%{_n2}%} with moveable air
                    else:
                        make gui slot {_n5::%{_n2}%} with moveable {tidegen.save::%loop-value-1%::%{_n2}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
    loop {drinkblender.location::*}:
        if gui with id "DrinkBlender.%loop-value%" is not set:
            create a gui with id "DrinkBlender.%loop-value%" with virtual chest inventory with 6 rows named "Drink Blender" with shape "1wyyybooo" and "2wy3ybo4o" and  "5wyyybooo" and "6wbbgggbb" and "7wbbg8gbb" and "9wbbgggbb":
                set {_n3} to 0
                loop 9 times:
                    add 1 to {_n3}
                    set {_n6::*} to 0, 9, 18, 27, 36, 45, 12, 16, and 41
                    if {drinkblender.save::%loop-value-1%::%{_n3}%} is not set:
                        make gui slot {_n6::%{_n3}%} with moveable air
                    if {drinkblender.save::%loop-value-1%::%{_n3}%} is air:
                        make gui slot {_n6::%{_n3}%} with moveable air
                    else:
                        make gui slot {_n6::%{_n3}%} with moveable {drinkblender.save::%loop-value-1%::%{_n3}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
                make gui slot "y" with yellow stained glass pane named "&eDrink Input"
                make gui slot "o" with orange stained glass pane named "&6Ingredient Input"
                make gui slot "b" with gray stained glass pane named " "
                make gui slot "g" with lime stained glass pane named "&aOutput Slot"
    loop {magnet.location::*}:
        if gui with id "Magnet.%loop-value%" is not set:
            create a gui with id "Magnet.%loop-value%" with virtual dropper named "Super Magnet" with shape "w_w" and "123" and "www":
                set {_n7} to 0
                loop 3 times:
                    add 1 to {_n7}
                    set {_n8::*} to 3, 4, and 5
                    if {magnet.save::%loop-value-1%::%{_n7}%} is not set:
                        make gui slot {_n8::%{_n7}%} with moveable air
                    if {magnet.save::%loop-value-1%::%{_n7}%} is air:
                        make gui slot {_n8::%{_n7}%} with moveable air
                    else:
                        make gui slot {_n8::%{_n7}%} with moveable {magnet.save::%loop-value-1%::%{_n7}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
    loop {engine.location::*}:
        if gui with id "SearingEngine.%loop-value%" is not set:
            create a gui with id "SearingEngine.%loop-value%" with virtual chest inventory with 3 rows named "Searing Engine" with shape "1wbooobbb" and "2wbo3obxb" and "4wbooobbb":
                set {_n10} to 0
                loop 4 times:
                    add 1 to {_n10}
                    set {_n9::*} to 0, 9, 18, and 13
                    if {engine.save::%loop-value-1%::%{_n10}%} is not set:
                        make gui slot {_n9::%{_n10}%} with moveable air
                    if {engine.save::%loop-value-1%::%{_n10}%} is air:
                        make gui slot {_n9::%{_n10}%} with moveable air
                    else:
                        make gui slot {_n9::%{_n10}%} with moveable {engine.save::%loop-value-1%::%{_n10}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
                make gui slot "o" with orange stained glass pane named "&6Fuel Input"
                make gui slot "b" with gray stained glass pane named " "
                        
on place of prismarine bricks:
    if nbt of player's held item contain "CheesyCraft:""DrinkBlender""":
        set {drinkblender.place::%event-block's location%} to true
        set {drinkblender.location::%event-block's location%} to event-block's location

on break of prismarine bricks:
    if {drinkblender.place::%event-block's location%} is true:
        cancel the drops
        clear {drinkblender.place::%event-block's location%}
        clear {drinkblender.location::%event-block's location%}
        clear {drinkblender.save::%event-block's location%::*}
        drop {saveditems::DrinkBlender}
        loop all items in gui with id "DrinkBlender.%event-block's location%":
            if index of loop-item is 0 or 9 or 18 or 27 or 36 or 45 or 12 or 16 or 41:
                drop loop-item
        delete gui with id "DrinkBlender.%event-block's location%"
        
on right click:
    if player is not sneaking:
        if event-player can build at location of event-block:
            if {drinkblender.place::%event-block's location%} is true:
                cancel event
                if gui with id "DrinkBlender.%event-block's location%" is not set:
                    create a gui with id "DrinkBlender.%event-block's location%" with virtual chest inventory with 6 rows named "Drink Blender" with shape "xwyyybooo" and "xwyxyboxo" and  "xwyyybooo" and "xwbbgggbb" and "xwbbgxgbb" and "xwbbgggbb":
                        make gui slot "x" with moveable air
                        make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
                        make gui slot "y" with yellow stained glass pane named "&eDrink Input"
                        make gui slot "o" with orange stained glass pane named "&6Ingredient Input"
                        make gui slot "b" with gray stained glass pane named " "
                        make gui slot "g" with lime stained glass pane named "&aOutput Slot"
                open gui with id "DrinkBlender.%event-block's location%" to player
                while name of player's current inventory contains "Drink Blender":
                    if event-block is air:
                        close player's inventory
                    wait 1 tick
                                        
on place of gilded blackstone:
    if nbt of player's held item contain "CheesyCraft:""PowerBankI""":
        set {powerbanki.place::%event-block's location%} to true
        set {powerbank.power::%event-block's location%} to 0
        set {powerbank.powertotal::%event-block's location%} to 800
        set {powerbank.location::%event-block's location%} to event-block's location
        set {powerbank.range::%event-block's location%} to 8
        if tag "Power" of nbt of player's held item is set:
            set {powerbank.power::%event-block's location%} to tag "Power" of nbt of player's held item

on break of gilded blackstone:
    if {powerbanki.place::%event-block's location%} is true:
        cancel the drops
        clear {powerbanki.place::%event-block's location%}
        set {_i} to {saveditems::powerbanki}
        if {powerbank.power::%event-block's location%} > 0:
            set line 6 of lore of {_i} to "%line 6 of lore of {_i}% &8Holding %{powerbank.power::%event-block's location%}% &8CP"
            add "{Power:%{powerbank.power::%event-block's location%}%}" to nbt of {_i}
        clear {powerbank.power::%event-block's location%}
        clear {powerbank.location::%event-block's location%}
        clear {powerbank.powertotal::%event-block's location%}
        clear {powerbank.range::%event-block's location%}
        drop {_i}
        loop all items in gui with id "powerbank.%event-block's location%":
            if index of loop-item is 3 or 4 or 5:
                drop loop-item
        delete gui with id "powerbank.%event-block's location%"
        
