every 1 second:
    loop {drinkblender.location::*}:
        if chunk at loop-value is loaded:
            if {drinkblender.maxed::%loop-value%} is true:
                continue loop
            loop (0, 9, 18, 27, 36, and 45):
                if nbt of slot loop-value-2 of gui with id "DrinkBlender.%loop-value-1%" contains "RedstoneSwitchMod":
                    if check [block at loop-value-1 is not redstone powered]->[block at loop-value-1 is not indirectly redstone powered]:
                        loop {powerbank.location::*}:
                            if distance between loop-value-1 and loop-value-3 <= {powerbank.range::%loop-value-3%}:
                                if {powerbank.power::%loop-value-3%} >= 10:
                                    set {_c1::*} to region at loop-value-1
                                    set {_c2::*} to region at loop-value-3
                                    if {_c1::*} = {_c2::*}:
                                        if nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%" contains "CheesyCraft:""SpookyCider""" or "CheesyCraft:""SpikedSpookyCider""":
                                            if slot 16 of gui with id "DrinkBlender.%loop-value-1%"'s type is potion:
                                                if slot 41 of gui with id "DrinkBlender.%loop-value-1%" is air:
                                                    set {_effects} to tag "Potion" of nbt of slot 16 of gui with id "DrinkBlender.%loop-value-1%"
                                                    replace "minecraft:" with "" in {_effects}
                                                    replace "_" with " " in {_effects}
                                                    set {_effects} to {_effects} in strict proper case
                                                    set {_i} to {saveditems::SpikedSpookyCider}
                                                    add 1 to {num::SpookyCider}
                                                    add "{PublicBukkitValues: {""ItemNumID"": %{num::SpookyCider}%}}" to nbt of {_i}
                                                    set line 18 of lore of {_i} to "&fSpiked with: &c%{_effects}%"
                                                    if tag "UsesLeft" of nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%" is set:
                                                        set {_value} to tag "UsesLeft" of nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                        set line 2 of lore of {_i} to line 2 of lore of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                        add "{UsesLeft:%{_value}%}" to nbt of {_i}
                                                    set slot 41 of gui with id "DrinkBlender.%loop-value-1%" to {_i}
                                                    set slot 16 of gui with id "DrinkBlender.%loop-value-1%" to glass bottle
                                                    remove 1 of slot 12 of gui with id "DrinkBlender.%loop-value-1%" from slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                    add -10 to {powerbank.power::%loop-value-3%} 
                                                    set {drinkblender.activated::%loop-value-1%} to true
                                                    set {drinkblender.activated2::%loop-value-1%} to true
                                                    exit loop
                                            else if nbt of slot 16 of gui with id "DrinkBlender.%loop-value-1%" contains "CheesyCraft:""LepusBrew""":
                                                if slot 41 of gui with id "DrinkBlender.%loop-value-1%" is air:
                                                    set {_i} to {saveditems::SpikedSpookyCider}
                                                    add 1 to {num::SpookyCider}
                                                    add "{PublicBukkitValues: {""ItemNumID"": %{num::SpookyCider}%}}" to nbt of {_i}
                                                    set line 18 of lore of {_i} to "&fSpiked with: %name of {saveditems::LepusBrew}%"
                                                    if tag "UsesLeft" of nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%" is set:
                                                        set {_value} to tag "UsesLeft" of nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                        set line 2 of lore of {_i} to line 2 of lore of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                        add "{UsesLeft:%{_value}%}" to nbt of {_i}
                                                    set slot 41 of gui with id "DrinkBlender.%loop-value-1%" to {_i}
                                                    remove 1 of slot 16 of gui with id "DrinkBlender.%loop-value-1%" from slot 16 of gui with id "DrinkBlender.%loop-value-1%"
                                                    remove 1 of slot 12 of gui with id "DrinkBlender.%loop-value-1%" from slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                    add -10 to {powerbank.power::%loop-value-3%} 
                                                    set {drinkblender.activated::%loop-value-1%} to true
                                                    set {drinkblender.activated2::%loop-value-1%} to true
                                                    exit loop
                                        if nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%" contains "CheesyCraft:""SummervineSoda""" or "CheesyCraft:""SpikedSummervineSoda""":
                                            if slot 16 of gui with id "DrinkBlender.%loop-value-1%"'s type is potion:
                                                if slot 41 of gui with id "DrinkBlender.%loop-value-1%" is air:
                                                    set {_effects} to tag "Potion" of nbt of slot 16 of gui with id "DrinkBlender.%loop-value-1%"
                                                    replace "minecraft:" with "" in {_effects}
                                                    replace "_" with " " in {_effects}
                                                    set {_effects} to {_effects} in strict proper case
                                                    set {_i} to {saveditems::SpikedSummervineSoda}
                                                    set line 9 of lore of {_i} to "&fSpiked with: &c%{_effects}%"
                                                    set slot 41 of gui with id "DrinkBlender.%loop-value-1%" to {_i}
                                                    set slot 16 of gui with id "DrinkBlender.%loop-value-1%" to glass bottle
                                                    remove 1 of slot 12 of gui with id "DrinkBlender.%loop-value-1%" from slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                    add -10 to {powerbank.power::%loop-value-3%} 
                                                    set {drinkblender.activated::%loop-value-1%} to true
                                                    set {drinkblender.activated2::%loop-value-1%} to true
                                                    exit loop
                                            else if nbt of slot 16 of gui with id "DrinkBlender.%loop-value-1%" contains "CheesyCraft:""LepusBrew""":
                                                if slot 41 of gui with id "DrinkBlender.%loop-value-1%" is air:
                                                    set {_i} to {saveditems::SpikedSummervineSoda}
