                                                    remove 1 of slot 12 of gui with id "DrinkBlender.%loop-value-1%" from slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                    add -10 to {powerbank.power::%loop-value-3%} 
                                                    set {drinkblender.activated::%loop-value-1%} to true
                                                    set {drinkblender.activated2::%loop-value-1%} to true
                                                    exit loop
                                        if 1 of slot 12 of gui with id "DrinkBlender.%loop-value-1%" is powder snow bucket:
                                            if 1 of slot 16 of gui with id "DrinkBlender.%loop-value-1%" is golden carrot:
                                                if 1 of slot 41 of gui with id "DrinkBlender.%loop-value-1%" is air or {saveditems::LepusBrew}:
                                                    if item amount of slot 41 of gui with id "DrinkBlender.%loop-value-1%" < 64:
                                                        set {_i} to {saveditems::LepusBrew}
                                                        add 1 of {_i} to slot 41 of gui with id "DrinkBlender.%loop-value-1%"
