                                                                set line 16 of lore of {_i} to line 16 of lore of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                            if nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%" contains "CheesyCraft:""AmericanDream""":
                                                                set line 16 of lore of {_i} to line 11 of lore of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
                                                            set {_value2} to tag "UsesLeft" of nbt of slot 12 of gui with id "DrinkBlender.%loop-value-1%"
