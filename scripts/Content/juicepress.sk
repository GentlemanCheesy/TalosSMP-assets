on unload:
    loop {JuicePress.location::*}:
        set {_n1} to 0
        loop 28 times:
            set {_n} to loop-number - 1
            if {_n} is 0 or 9 or 27 or 18 or 12 or 16:
                add 1 to {_n1}
                set {JuicePress.save::%loop-value-1%::%{_n1}%} to slot {_n} of gui with id "JuicePress.%loop-value-1%"

on load:
    loop {JuicePress.location::*}:
        if gui with id "JuicePress.%loop-value%" is not set:
            create a gui with id "JuicePress.%loop-value%" with virtual chest inventory with 4 rows named "Juice Press" with shape "1wyyybooo" and "2wy3ybo4o" and "5wyyybooo" and "6wbbbbbbb":
                set {_n1} to 0
                loop 6 times:
                    add 1 to {_n1}
                    set {_n4::*} to 0, 9, 18, 27, 12, and 16
                    if {JuicePress.save::%loop-value-1%::%{_n1}%} is not set:
                        make gui slot {_n4::%{_n1}%} with moveable air
                    if {JuicePress.save::%loop-value-1%::%{_n1}%} is air:
                        make gui slot {_n4::%{_n1}%} with moveable air
                    else:
                        make gui slot {_n4::%{_n1}%} with moveable {JuicePress.save::%loop-value-1%::%{_n1}%}
                make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
                make gui slot "y" with blue stained glass pane named "&9Input"
                make gui slot "o" with lime stained glass pane named "&aOutput"
                make gui slot "b" with gray stained glass pane named " "

on place of magenta terracotta:
    if nbt of player's held item contains "CheesyCraft:""JuicePress""":
        set {JuicePress.place::%event-block's location%} to true
        set {JuicePress.location::%event-block's location%} to event-block's location

on break of magenta terracotta:
    if {JuicePress.place::%event-block's location%} is true:
        cancel the drops
        clear {JuicePress.place::%event-block's location%}
        clear {JuicePress.location::%event-block's location%}
        drop {saveditems::JuicePress}
        loop all items in gui with id "JuicePress.%event-block's location%":
            if index of loop-item is 0 or 9 or 27 or 18 or 12 or 16:
                drop loop-item
        delete gui with id "JuicePress.%event-block's location%"
        
on right click:
    if player is not sneaking:
        if event-player can build at location of event-block:
            if {JuicePress.place::%event-block's location%} is true:
                cancel event
                if gui with id "JuicePress.%event-block's location%" is not set:
                    create a gui with id "JuicePress.%event-block's location%" with virtual chest inventory with 4 rows named "Juice Press" with shape "xwyyybooo" and "xwyxyboxo" and "xwyyybooo" and "xwbbbbbbb":
                        make gui slot "x" with moveable air
                        make gui slot "w" with white stained glass pane named "&fMachine Modifiers"
                        make gui slot "y" with blue stained glass pane named "&9Input"
                        make gui slot "o" with lime stained glass pane named "&aOutput"
                        make gui slot "b" with gray stained glass pane named " "
                open gui with id "JuicePress.%event-block's location%" to player
                while name of player's current inventory contains "Electric JuicePress":
                    if event-block is air:
                        close player's inventory
                    wait 1 tick
            

every 1 second:
