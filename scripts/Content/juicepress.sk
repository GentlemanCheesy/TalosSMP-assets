    loop {JuicePress.location::*}:
        if {juicepress.activated::%loop-value%} is true:
            if chunk at loop-value is loaded:
                loop 28 times:
                    set {_n} to loop-number - 1
                    if {_n} is 0 or 9 or 18 or 27:
                        if nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%" contains "CheesyCraft:""EcholomaticMod""":
                            loop all blocks in radius 9 of loop-value-1:
                                if loop-value-3 is sculk sensor:
                                    spawn snowball at location of loop-value-3
                            delete {juicepress.activated::%loop-value-1%}
                            exit loop
                                  
every 2 ticks:
    loop {JuicePress.location::*}:
        if chunk at loop-value is loaded:
            loop 28 times:
                set {_n} to loop-number - 1
                if {_n} is 0 or 9 or 18 or 27:
                    if nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%" contains "CheesyCraft:""WardstaticMod""":
                        set {_total} to tag "Total" of nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%"
                        set {_max} to tag "Max" of nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%"
                        if {_total} = {_max}:
                            set {JuicePress.maxed::%loop-value-1%} to true
                        if {_total} < {_max}:
                            if {JuicePress.activated2::%loop-value-1%} is true:
                                add "{Total:%{_total} + 1%}" to nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%"
                        loop all blocks in radius 1 of loop-value-1:
                            if loop-value-3 is sculk:
                                delete {JuicePress.maxed::%loop-value-1%}
                                add "{Total:0}" to nbt of slot {_n} of gui with id "JuicePress.%loop-value-1%"
                        delete {JuicePress.activated2::%loop-value-1%}
                
